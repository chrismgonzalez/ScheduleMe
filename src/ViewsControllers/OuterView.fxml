<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="ViewsControllers.OuterController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <BorderPane prefHeight="800.0" prefWidth="1280.0">
            <left>
                <ToolBar orientation="VERTICAL" BorderPane.alignment="CENTER">
                    <items>
                        <Button fx:id="scheduleViewButton" mnemonicParsing="false" onAction="#launchScheduleScene" prefHeight="60.0" prefWidth="75.0" text="Schedule" />
                        <Button fx:id="customersViewButton" mnemonicParsing="false" onAction="#launchCustomersScene" prefHeight="60.0" prefWidth="75.0" text="Customers" />
                        <Button fx:id="reportsViewButton" mnemonicParsing="false" onAction="#launchReportsScene" prefHeight="60.0" prefWidth="75.0" text="Reports" />
                        <Button fx:id="logoutViewButton" mnemonicParsing="false" onAction="#launchLoginScene" prefHeight="60.0" prefWidth="75.0" text="Logout" />
                    </items>
                    <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </padding>
                </ToolBar>
            </left>
            <center>
                <TitledPane fx:id="innerSceneTitledPane" alignment="CENTER" animated="false" collapsible="false" contentDisplay="TEXT_ONLY" prefHeight="800.0" prefWidth="1200.0" text="Schedule" BorderPane.alignment="CENTER">
                    <content>
                        <AnchorPane />
                    </content>
                </TitledPane>
            </center>
        </BorderPane>
    </children>
</AnchorPane>
