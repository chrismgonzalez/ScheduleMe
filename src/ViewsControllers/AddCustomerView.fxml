<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="ViewsControllers.AddCustomerView"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <TitledPane alignment="CENTER" collapsible="false" prefWidth="600.0" text="New Customer" textAlignment="CENTER">
            <content>
                <VBox prefHeight="400.0" prefWidth="600.0" spacing="10.0">
                    <children>
                        <GridPane prefHeight="318.0" prefWidth="390.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                            <children>
                                <Label text="Name:" GridPane.rowIndex="0" />
                                <Label text="Active:" GridPane.rowIndex="1" />
                                <Label text="Phone:" GridPane.rowIndex="3" />
                                <Label text="Address:" GridPane.rowIndex="4" />
                                <Label text="Address Two:" GridPane.rowIndex="5" />
                                <Label text="City:" GridPane.rowIndex="6" />
                                <Label text="Postal Code:" GridPane.rowIndex="7" />
                                <Label text="Country:" GridPane.rowIndex="8" />
                                <TextField fx:id="customerNameTextField" GridPane.columnIndex="1" />
                                <CheckBox fx:id="customerActiveCheckBox" mnemonicParsing="false" selected="true" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                                <TextField fx:id="customerPhoneTextField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                                <TextField fx:id="customerAddressTextField" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                                <TextField fx:id="customerAddress2TextField" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                                <TextField fx:id="customerPostalCodeTextField" GridPane.columnIndex="1" GridPane.rowIndex="7" />
                                <TextField fx:id="customerCountryTextField" GridPane.columnIndex="1" GridPane.rowIndex="8" />
                                <TextField fx:id="customerCityTextField" layoutX="304.0" layoutY="269.0" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                            </children>
                        </GridPane>
                        <HBox alignment="CENTER_RIGHT" prefHeight="35.0" prefWidth="700.0" spacing="10.0">
                            <padding>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                            </padding>
                            <children>
                                <Button fx:id="saveNewCustomerButton" mnemonicParsing="false" onAction="#saveNewCustomer" text="Add Customer" />
                                <Button fx:id="cancelNewCustomerButton" mnemonicParsing="false" onAction="#cancelNewCustomer" text="Cancel" />
                            </children>
                        </HBox>
                    </children>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </padding>
                    <opaqueInsets>
                        <Insets top="5.0" />
                    </opaqueInsets>
                </VBox>
            </content>
        </TitledPane>
    </children>
</AnchorPane>
