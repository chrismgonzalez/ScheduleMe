<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane minHeight="-Infinity" prefHeight="900.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ViewsControllers.OuterController">
    <children>
        <BorderPane prefHeight="800.0" prefWidth="1280.0">
            <center>
                <TitledPane fx:id="innerSceneTitledPane" alignment="CENTER" animated="false" collapsible="false" contentDisplay="TEXT_ONLY" prefHeight="800.0" prefWidth="1200.0" text="Schedule" BorderPane.alignment="CENTER">
                    <content>
                        <AnchorPane />
                    </content>
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
                </TitledPane>
            </center>
         <top>
            <ButtonBar buttonMinWidth="75.0" prefHeight="84.0" prefWidth="1176.0" BorderPane.alignment="CENTER">
              <buttons>
                        <Button fx:id="scheduleViewButton" mnemonicParsing="false" onAction="#launchScheduleScene" prefHeight="36.0" prefWidth="279.0" style="-fx-background-color: lightblue; -fx-border-color: grey; -fx-border-radius: 2; -fx-border-width: 1
                        ;" text="Schedule">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font></Button>
                        <Button fx:id="customersViewButton" mnemonicParsing="false" onAction="#launchCustomersScene" prefHeight="36.0" prefWidth="75.0" style="-fx-background-color: lightblue; -fx-border-color: grey; -fx-border-radius: 2; -fx-border-width: 1" text="Customers">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font></Button>
                        <Button fx:id="reportsViewButton" mnemonicParsing="false" onAction="#launchReportsScene" prefHeight="36.0" prefWidth="75.0" style="-fx-background-color: lightblue; -fx-border-color: grey; -fx-border-radius: 2; -fx-border-width: 1" text="Reports">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font></Button>
                        <Button fx:id="logoutViewButton" mnemonicParsing="false" onAction="#launchLoginScene" prefHeight="36.0" prefWidth="206.0" style="-fx-background-color: lightblue; -fx-border-color: grey; -fx-border-radius: 2; -fx-border-width: 1" text="Logout">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font></Button>
              </buttons>
               <padding>
                  <Insets right="75.0" />
               </padding>
            </ButtonBar>
         </top>
        </BorderPane>
    </children>
</AnchorPane>
